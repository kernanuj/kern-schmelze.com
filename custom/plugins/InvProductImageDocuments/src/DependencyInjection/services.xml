           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <imports>
        <import resource="translations.xml" />
        <import resource="decorators.xml" />
    </imports>

    <services>
        <service id="InvProductImageDocuments\Service\Configuration">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService"/>
        </service>

        <service class="InvProductImageDocuments\Service\DocumentService" id="inv.article_image_bills.service.document_service">
            <argument type="service" id="Shopware\Core\Content\Media\DataAbstractionLayer\MediaRepositoryDecorator"/>
        </service>
    </services>
</container>
