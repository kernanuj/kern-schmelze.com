{% if shopware.config.InvHomepageSlideshowPro.config.status == true and shopware.config.InvHomepageSlideshowPro.config.image1 and activeRoute == "frontend.home.page" %}
    <section class="center slider" data-autoplay="{{ shopware.config.InvHomepageSlideshowPro.config.autoplay }}" data-autoplay-speed="{{ shopware.config.InvHomepageSlideshowPro.config.autoplayspeed }}">
        {%  if shopware.config.InvHomepageSlideshowPro.config.image1 %}
            {% set media = searchMedia([shopware.config.InvHomepageSlideshowPro.config.image1], context.context) %}
            {% set image1Entity = media.get(shopware.config.InvHomepageSlideshowPro.config.image1) %}
            {% set media = searchMedia([shopware.config.InvHomepageSlideshowPro.config.image11], context.context) %}
            {% set image11Entity = media.get(shopware.config.InvHomepageSlideshowPro.config.image11) %}

            {% if shopware.config.InvHomepageSlideshowPro.config.image11m %}
                {% set media = searchMedia([shopware.config.InvHomepageSlideshowPro.config.image11m], context.context) %}
                {% set image11mEntity = media.get(shopware.config.InvHomepageSlideshowPro.config.image11m) %}
                {% set image11mStyle = "background-image: url("~image11mEntity.url~");" %}
            {% else %}
                {% set image11mStyle = "background-image: url("~image1Entity.url~");" %}
            {% endif %}

            <div class="inv-slider-item inv-slider-item-1">
                <div class="row">
                    <div class="col-lg-6 col-md-6 left-slide desktop" style="background-image: url({{ image1Entity.url }});">
                        <div class="inv-slider-item-content-holder">
                            {{ shopware.config.InvHomepageSlideshowPro.config.slide1|raw }}
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-6 left-slide mobile" style="background-image: url({{ image11mEntity.url }});">
                        <div class="inv-slider-item-content-holder">
                            {{ shopware.config.InvHomepageSlideshowPro.config.slide1|raw }}
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-6 right-slide hidden-md hidden-sm hidden-xs" style="background-image: url({{ image11Entity.url }});"></div>
                </div>
            </div>
        {% endif %}

