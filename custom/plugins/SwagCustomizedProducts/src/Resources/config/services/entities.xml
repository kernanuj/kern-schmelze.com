<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <!-- Definitions -->
        <service id="Swag\CustomizedProducts\Template\TemplateDefinition">
            <tag name="shopware.entity.definition" entity="swag_customized_products_template"/>
        </service>

        <service id="Swag\CustomizedProducts\Template\SalesChannel\SalesChannelTemplateDefinition">
            <tag name="shopware.sales_channel.entity.definition"/>
        </service>

        <service id="Swag\CustomizedProducts\Template\Aggregate\TemplateOption\TemplateOptionDefinition">
            <tag name="shopware.entity.definition" entity="swag_customized_products_template_option"/>
        </service>

        <service id="Swag\CustomizedProducts\Template\Aggregate\TemplateOption\SalesChannel\SalesChannelTemplateOptionDefinition">
            <tag name="shopware.sales_channel.entity.definition"/>
        </service>

        <service id="Swag\CustomizedProducts\Template\Aggregate\TemplateOptionPrice\TemplateOptionPriceDefinition">
            <tag name="shopware.entity.definition" entity="swag_customized_products_template_option_price"/>
        </service>

        <service id="Swag\CustomizedProducts\Template\Aggregate\TemplateOptionValue\TemplateOptionValueDefinition">
            <tag name="shopware.entity.definition" entity="swag_customized_products_template_option_value"/>
        </service>

        <service id="Swag\CustomizedProducts\Template\Aggregate\TemplateOptionValue\SalesChannel\SalesChannelTemplateOptionValueDefinition">
            <tag name="shopware.sales_channel.entity.definition"/>
        </service>

        <service id="Swag\CustomizedProducts\Template\Aggregate\TemplateOptionValuePrice\TemplateOptionValuePriceDefinition">
            <tag name="shopware.entity.definition" entity="swag_customized_products_template_option_value_price"/>
        </service>

        <service id="Swag\CustomizedProducts\Template\Aggregate\TemplateExclusion\TemplateExclusionDefinition">
            <tag name="shopware.entity.definition" entity="swag_customized_products_template_exclusion"/>
        </service>

        <service id="Swag\CustomizedProducts\Template\Aggregate\TemplateExclusionCondition\TemplateExclusionConditionDefinition">
            <tag name="shopware.entity.definition" entity="swag_customized_products_template_exclusion_condition"/>
        </service>

        <service id="Swag\CustomizedProducts\Template\Aggregate\TemplateExclusionOperator\TemplateExclusionOperatorDefinition">
            <tag name="shopware.entity.definition" entity="swag_customized_products_template_exclusion_operator"/>
        </service>

        <service id="Swag\CustomizedProducts\Template\Aggregate\TemplateExclusionConditionValues\TemplateExclusionConditionValuesDefinition">
            <tag name="shopware.entity.definition" entity="swag_customized_products_template_exclusion_condition_values"/>
        </service>

        <!-- Translations -->
        <service id="Swag\CustomizedProducts\Template\Aggregate\TemplateTranslation\TemplateTranslationDefinition">
            <tag name="shopware.entity.definition" entity="swag_customized_products_template_translation"/>
        </service>

        <service id="Swag\CustomizedProducts\Template\Aggregate\TemplateOptionTranslation\TemplateOptionTranslationDefinition">
            <tag name="shopware.entity.definition" entity="swag_customized_products_template_option_translation"/>
        </service>

        <service id="Swag\CustomizedProducts\Template\Aggregate\TemplateOptionValueTranslation\TemplateOptionValueTranslationDefinition">
            <tag name="shopware.entity.definition" entity="swag_customized_products_template_option_value_translation"/>
        </service>

        <service id="Swag\CustomizedProducts\Template\Aggregate\TemplateExclusionOperatorTranslation\TemplateExclusionOperatorTranslationDefinition">
            <tag name="shopware.entity.definition" entity="swag_customized_products_template_exclusion_operator_translation"/>
        </service>
    </services>
</container>
