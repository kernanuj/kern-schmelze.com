{
    "name": "swag/cms-extensions",
    "description": "CMS extensions for Shopware 6",
    "version": "1.7.2",
    "type": "shopware-platform-plugin",
    "license": "proprietary",
    "authors": [
        {
            "name": "Shopware"
        }
    ],
    "require": {
        "shopware/core": "~6.3.4",
        "shopware/storefront": "~6.3.4"
    },
    "conflict": {
        "shopware/storefront": "<6,>=7",
        "shopware/administration": "<6,>=7"
    },
    "extra": {
        "shopware-plugin-class": "Swag\\CmsExtensions\\SwagCmsExtensions",
        "copyright": "(c) by shopware AG",
        "label": {
            "de-DE": "CMS-Erweiterungen für Shopware 6",
            "en-GB": "CMS extensions for Shopware 6"
        },
        "description": {
            "de-DE": "Includes several features for the CMS of Shopware 6",
            "en-GB": "Enthält diverse Funktionen für das CMS von Shopware 6"
        },
        "manufacturerLink": {
            "de-DE": "https://store.shopware.com/shopware-ag.html",
            "en-GB": "https://store.shopware.com/en/shopware-ag.html"
        },
        "supportLink": {
            "de-DE": "https://issues.shopware.com/",
            "en-GB": "https://issues.shopware.com/"
        }
    },
    "autoload": {
        "psr-4": {
            "Swag\\CmsExtensions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swag\\CmsExtensions\\Test\\": "tests/"
        }
    },
    "suggest": {
        "shopware/administration": "Allows you to to set up the plugin easily"
    }
}
