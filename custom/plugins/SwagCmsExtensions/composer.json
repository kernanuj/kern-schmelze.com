{
    "name": "swag/cms-extensions",
    "description": "CMS extensions for Shopware 6",
    "version": "1.4.0",
    "type": "shopware-platform-plugin",
    "license": "proprietary",
    "authors": [
        {
            "name": "shopware AG"
        }
    ],
    "require": {
        "shopware/core": "~6.2.1",
        "shopware/storefront": "~6.2.1"
    },
    "extra": {
        "shopware-plugin-class": "Swag\\CmsExtensions\\SwagCmsExtensions",
        "copyright": "(c) by shopware AG",
        "label": {
            "de-DE": "CMS-Erweiterungen f√ºr Shopware 6",
            "en-GB": "CMS extensions for Shopware 6"
        }
    },
    "autoload": {
        "psr-4": {
            "Swag\\CmsExtensions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swag\\CmsExtensions\\Test\\": "tests/"
        }
    },
    "suggest": {
        "shopware/administration": "Allows you to to set up the plugin easily"
    },
    "conflict": {
        "shopware/storefront": "<6,>=7",
        "shopware/administration": "<6,>=7"
    }
}
